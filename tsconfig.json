  "compilerOptions": {
    "baseUrl": ".",
    "target": "ESNext",
    "module": "node",
    "moduleResolution": "node",
    "strict": true,
    "isolatedModules": true,
    "verbatimModuleSyntax": true,
    "skipLibCheck": true,
    "types": [
      "node"
    ]
  },
  "exclude": ["/generated/"]
}
